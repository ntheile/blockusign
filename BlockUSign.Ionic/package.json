{
  "name": "blockusign",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "main": "www/main.js",
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "copySignUpPage": "cp ./src/signup.html ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/",
    "copyBadgePage": "cp ./src/badge.html ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/",
    "buildprod": "npm run dev && npm run makewwwroot --silent && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage && npm run copyBadgePage",
    "buildweb": "ionic build && npm run makewwwroot --silent && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage && npm run copyBadgePage",
    "prod": "ionic build --prod --aot --minifyjs --minifycss --optimizejs",
    "dev": "ionic build",
    "makewwwroot": "cd ../BlockUSign.Backend/BlockUSign.Backend/ && [ -d wwwroot ] || mkdir wwwroot",
    "fast": "node ./scripts/server.js",
    "chrome": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome.ps1",
    "chrome-private": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome-private.ps1",
    "chrome-mac": "./scripts/chrome.sh",
    "chrome-mac-private": "./scripts/chrome-private.sh",
    "serve": "ionic build && node ./scripts/server.js",
    "private": "npm run chrome-mac-private && ionic serve",
    "start": "npm run start:windows",
    "start:windows": "npm run chrome && ionic serve",
    "start:mac": "npm run chrome-mac && ionic serve",
    "start:mac:private": "npm run chrome-mac-private && ionic serve",
    "electron": "ionic-app-scripts build --copy electron-copy-files-script.js && electron .",
    "test": "karma start ./test-config/karma.conf.js",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false --ignore_ssl",
    "deploy": "",
    "deploy:test": "",
    "ugly": "uglifyjs src/vendor/blockstack.js src/vendor/babel-polyfill.min.js src/vendor/pdf.js src/vendor/jquery.js src/vendor/pdf_viewer.js src/vendor/text_layer_builder.js src/vendor/spectrum.js src/vendor/sjcl.js src/vendor/pdf-annotate.js src/vendor/bitcoinjs-lib.js src/vendor/parseZoneFile.js src/vendor/interact.min.js src/vendor/ccmArrayBuffer.js src/vendor/codecArrayBuffer.js src/vendor/jslinq.min.js src/vendor/jquery.blast.min.js src/vendor/pdfkit.js src/vendor/jspdf.debug.js src/vendor/webfont.js src/vendor/fuse.min.js -o src/assets/ugly.min.js",
    "uglycss": "uglifycss src/assets/css/ubuntu-font.css src/assets/css/cursive-font.css src/assets/css/pdf_viewer.css src/assets/text_layer_builder.css src/assets/spectrum.css src/assets/css/glegoo-font.css src/assets/ng-select.material.theme.css --output src/assets/ugly.min.css"
  },
