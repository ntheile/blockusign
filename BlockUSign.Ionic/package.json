{
  "name": "blockusign",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "main": "www/main.js",
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "copySignUpPage": "cp ./src/signup.html ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/",
    "buildprod": "npm run prod && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage",
    "buildweb": "ionic build && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage",
    "prod": "ionic build --prod --aot --minifyjs --minifycss --optimizejs",
    "fast": "node ./scripts/server.js",
    "chrome": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome.ps1",
    "chrome-private": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome-private.ps1",
    "chrome-mac": "./scripts/chrome.sh",
    "chrome-mac-private": "./scripts/chrome-private.sh",
    "serve": "ionic build && node ./scripts/server.js",
    "private": "npm run chrome-mac-private && ionic serve",
    "start": "npm run start:windows",
    "start:windows": "npm run chrome && ionic serve",
    "start:mac": "npm run chrome-mac && ionic serve",
    "start:mac:private": "npm run chrome-mac-private && ionic serve",
    "electron": "ionic-app-scripts build --copy electron-copy-files-script.js && electron .",
    "test": "karma start ./test-config/karma.conf.js",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false --ignore_ssl"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ng-select/ng-select": "^1.5.0",
    "@rdfjs/parser-jsonld": "^1.1.1",
    "@types/underscore": "^1.8.8",
    "automerge": "^0.7.8",
    "babel-polyfill": "^6.26.0",
    "bitcoinjs-lib": "^4.0.1",
    "bitcore-explorers": "^1.0.1",
    "bitcore-lib": "^0.15.0",
    "blockstack.ts": "^0.1.0",
    "bower": "^1.8.2",
    "chart.js": "^2.7.1",
    "cordova-android": "6.3.0",
    "cordova-plugin-device": "^1.1.4",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-whitelist": "^1.3.1",
    "cors": "^2.8.4",
    "eosjs": "^15.0.3",
    "eosjs-keygen": "^1.3.2",
    "express": "^4.16.3",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "jslinq": "^1.0.21",
    "linq-to-typescript": "^2.0.0",
    "linqts": "^1.11.0",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.14",
    "ng2-charts": "^1.6.0",
    "node-localstorage": "^1.3.1",
    "nvm": "0.0.4",
    "openpgp": "^3.0.10",
    "opn": "^5.3.0",
    "pdf-annotate": "0.0.0",
    "pdfjs-dist": "^2.0.480",
    "requirejs": "^2.3.5",
    "rxjs": "^5.5.2",
    "rxjs-compat": "^6.1.0",
    "sw-toolbox": "3.6.0",
    "technicalindicators": "^1.1.1",
    "underscore": "^1.9.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.2.0",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.0",
    "angular2-template-loader": "^0.6.2",
    "argv": "0.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^2.0.8",
    "html-loader": "^0.5.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^2.99.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "null-loader": "^0.1.1",
    "protractor": "^5.4.1",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "2.4.2"
  },
  "description": "Blockusign - A decentralized, social document signing tool where you own and control your own documents, contracts and data.",
  "cordova": {
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "android"
    ]
  },
  "build": {
    "appId": "co.blockusign",
    "files": [
      "electron/main.js",
      "www/**/*"
    ],
    "mac": {
      "category": "productivity"
    }
  }
}
