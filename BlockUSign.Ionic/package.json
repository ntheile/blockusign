  "description": "Blockusign - A decentralized, social document signing tool where you own and control your own documents, contracts and data.",
  "cordova": {
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "android"
    ]
  },
  "build": {
    "appId": "co.blockusign",
    "files": [
      "electron/main.js",
      "www/**/*"
    ],
    "mac": {
      "category": "productivity"
    }
  }
